{
    "meta": {
        "label": "SAP OData",
        "description": "Import data from a SAP/OData account",
        "icon": "icon-list"
    },
    "readable": true,
    "writable": false,
    "params": [
        {
            "label": "Authentication",
            "type": "SEPARATOR"
        },
        {
            "name": "auth_type",
            "label": "Type of authentication",
            "type": "SELECT",
            "defaultValue": "login",
            "visibilityCondition": true,
            "selectChoices": [
                {
                    "value": "user-account",
                    "label": "Personal user account"
                },
                {
                    "value": "login",
                    "label": "Service account"
                }
            ]
        },
        {
            "name": "sap-odata_login",
            "label": "OData preset",
            "type": "PRESET",
            "parameterSetId": "login",
            "visibilityCondition": "model.auth_type == 'login'"
        },
        {
            "name": "sap-odata_user-account",
            "label": "Personal user account",
            "type": "PRESET",
            "parameterSetId": "user-accounts",
            "visibilityCondition": "model.auth_type == 'user-account'"
        },
        {
            "label": "Data to retrieve",
            "type": "SEPARATOR"
        },
        {
            "name": "odata_service_node",
            "label": "Service name",
            "description": "",
            "type": "STRING",
            "defaultValue": "",
            "mandatory": false
        },
        {
            "name": "odata_list_title",
            "label": "Entity",
            "description": "",
            "type": "STRING",
            "defaultValue": "",
            "mandatory": false
        },
        {
            "name": "show_advanced_parameters",
            "label": " ",
            "description": "Show advanced parameters",
            "type": "BOOLEAN",
            "defaultValue": false
        },
        {
            "label": "Advanced parameters",
            "type": "SEPARATOR",
            "visibilityCondition": "model.show_advanced_parameters == true"
        },
        {
            "name": "odata_filter_query",
            "label": "Filter query",
            "description": "",
            "type": "STRING",
            "defaultValue": "",
            "visibilityCondition": "model.show_advanced_parameters == true",
            "mandatory": false
        },
        {
            "name": "should_convert_date",
            "label": " ",
            "description": "Convert date",
            "type": "BOOLEAN",
            "defaultValue": true,
            "visibilityCondition": "model.show_advanced_parameters == true"
        },
        {
            "name": "bulk_size",
            "label": " ",
            "description": "Bulk size",
            "type": "INT",
            "defaultValue": 1000,
            "visibilityCondition": "model.show_advanced_parameters == true"
        }
    ]
}